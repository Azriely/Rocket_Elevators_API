<!-- Google Maps API Testing -->
<div id="map" style="width:100%; height:700px;"></div>

<script defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjhXlHWWys4hFTzaG6Jd8UwTpLWIAKpqM&callback=initMap">
// ENV['mapsAPI_id']
</script>

<script>

   function initMap(){

	// options
	var options = {
		center: {lat:45, lng: -99},
		zoom: 5
	}

	// new map
  	map = new google.maps.Map(document.getElementById('map'),options)

	//marker
	function addMarker(property){
		const marker = new google.maps.Marker({
			position:property.location,
			map:map
		});

	}
   addMarker({location:{lat: 40.7128, lng: -74.0060}})
   addMarker({location:{lat: 27.9506, lng: -82.4572}})
   addMarker({location:{lat: 39.0997, lng: -94.5786}})
   }
</script>
<!-- /Google Maps API Testing -->
